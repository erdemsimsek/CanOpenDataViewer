{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug canopend",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/canopend",
            "args": [
                "can0",
                "-i", "4",
                "-c", "local-/tmp/CO_command_socket"
            ],
            "preLaunchTask": "Delete socket and Build",
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb"
        }
    ]
}
